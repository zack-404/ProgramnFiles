		Love();
	}
	
	static void sol1() {
		Scanner sc=new Scanner(System.in);
		String ln1=sc.next();
		char arr1[]=ln1.toCharArray();
		
	}

	static void love() {
		char arr1[]= {'l','o','v','e'};
		int arr2[]=new int[4];
		for(int i=0;i<4;i++) {
			arr2[i]=arr1[i];
			System.out.println(arr2[i]);
		}
		/*
		 	108
			111
			118
			101

		 */
		
	}
	
	static void Love() {
		char arr1[]= {'L','o','v','e'};
		int arr2[]=new int[4];
		for(int i=0;i<4;i++) {
			arr2[i]=arr1[i];
			System.out.println(arr2[i]);
		}
	}
	

package a__;

import java.util.Scanner;

public class a628 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		String ln="\t";
		System.out.println(ln);
		//sol1();
	}
	
	static void sol1() {
		Scanner sc=new Scanner(System.in);
		int n=sc.nextInt(),arr1[][]=new int[n][n],count=n*n-1;
		for(int j=0;n>n/2+1;n--,j++) {
			for(int i=n-1;i>-1;i--,count--) {
				arr1[n-1][i]=count;
				System.out.print(count);
			}
			count++;
			for(int i=n-1;i>-1;i--,count--) {
				arr1[i][j]=count;
				System.out.print(count);
			}
			count++;
			for(int i=0;i<n;i++,count--) {
				arr1[j][i]=count;
				System.out.print(count);
			}
			count++;
			for(int i=0;i<n-1;i++,count--) {
				arr1[n-1][i]=count;
				System.out.print(count);
			}
			count++;
		}
		
		for(int x[]:arr1) {
			for(int y:x)
				System.out.print(y+"\t"
						+ "");
			System.out.println();
		}
	}

}
